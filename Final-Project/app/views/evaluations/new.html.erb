<h1>New Evaluation for <%= @presentation.title %></h1>
<%= form_with model: [@presentation, @evaluation] do |f| %>
  <div>
    <%= f.label :score %>
    <%= f.number_field :score, in: 1..5 %>
  </div>
  <div>
    <%= f.label :comment %>
    <%= f.text_area :comment %>
  </div>
  <%= f.submit "Add Evaluation" %>
<% end %>
